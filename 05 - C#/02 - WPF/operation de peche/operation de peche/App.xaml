<Application x:Class="operation_de_peche.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:operation_de_peche"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <SolidColorBrush x:Key="disabled" Color="#FFF0F0F0"/>
        <SolidColorBrush x:Key="TxtRouge" Color="Red"/>
        <SolidColorBrush x:Key="BleuSection" Color="#FF2E5C95"/>
        <LinearGradientBrush x:Key="linearSection" EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
            <GradientStop Color="#FF3C6A95" Offset="1"/>
            <GradientStop Color="White" Offset="0.996"/>
            <GradientStop Color="#FFBBCBDA"/>
        </LinearGradientBrush>

    </Application.Resources>
</Application>
